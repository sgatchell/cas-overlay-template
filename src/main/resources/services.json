{
  /*
    allow all services to register with CAS
  */
  "@class" : "org.apereo.cas.services.RegexRegisteredService",
  "serviceId" : "^(https|http)://.*",
  "name" : "HTTPS and HTTP",
  "id" : 12300001,
  "attributeReleasePolicy" : {
    "@class" : "org.apereo.cas.services.ReturnAllowedAttributeReleasePolicy",
    "allowedAttributes" : [ "java.util.ArrayList", ["authId", "displayName", "emailAddress", "firstName", "groups", "lastName", "middleName", "nedId"]]
  }
}
